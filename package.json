{
  "name": "iportfolio",
  "version": "1.0.0",
  "description": "My Personal online Portfolio",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Khang-Tran/iPortfolio.git"
  },
  "author": "Khang Tran",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Khang-Tran/iPortfolio/issues"
  },
  "homepage": "https://github.com/Khang-Tran/iPortfolio#readme",
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "mongoose": "^5.0.13",
    "multer": "^1.3.0",
    "nodemailer": "^4.6.4",
    "styled-components": "^3.3.3"
  }
}
